      "target": [
        "dmg"
      ],
      "darkModeSupport": true,
      "icon": "assets/icon.icns"
    }
  },
  "devDependencies": {
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1"
  },
  "dependencies": {
    "marked": "^15.0.7",
    "openai": "^4.86.1"
  }
}
