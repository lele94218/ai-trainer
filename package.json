{
  "name": "ai-trainer",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "NODE_ENV=development electron .",
    "dev": "NODE_ENV=development electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.terryx.ai-trainer",
    "productName": "AI Trainer",
    "mac": {
